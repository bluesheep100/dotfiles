super + shift + s
    maim -s -u | xclip -selection clipboard -t image/png -i

super + n
    alacritty -e nvim /home/bluesheep/Notes/notes.md

super + m
    alacritty -e /usr/bin/neomutt

super + p
    phpstorm_pick

super + z
    zatpick

super + w
    wp_pick

XF86AudioMute
    pulseaudio-ctl mute

XF86AudioRaiseVolume
    pulseaudio-ctl up 5%

XF86AudioLowerVolume
    pulseaudio-ctl down 5%

XF86MonBrightnessUp
    brightnessctl set +5%

XF86MonBrightnessDown
    brightnessctl set 5%-

